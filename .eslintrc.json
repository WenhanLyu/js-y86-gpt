{
  "overrideConfig": {
    "linterOptions": {
      "reportUnusedDisableDirectives": true
    }
  },
  "rules": {
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "jsx-quotes": [
      "error",
      "prefer-single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
